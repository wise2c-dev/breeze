- name: clean docker
  hosts: hosts
  user: root
  tasks:
  - name: remove docker
    yum:
      disablerepo: '*'
      enablerepo: wise2c
      state: absent
      name: '{{ item }}'
    with_items:
    - rsync
    - jq
    - chrony
    - docker
    - python-docker-py
    - python-jinja2
    - docker-compose