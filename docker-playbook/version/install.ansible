- name: init host
  hosts: hosts
  user: root
  any_errors_fatal: true
  vars:
    path: /var/tmp/wise2c/docker

  tasks:
  - name: check docker
    script: scripts/check_docker.sh {{ harbor }}
    register: check_output

  - name: setup docker on all nodes
    include_tasks: docker.ansible
    when: check_output.stdout != 'true'
